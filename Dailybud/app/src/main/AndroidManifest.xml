<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.keerthana.dailybud">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".selection">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Todayexp" />
        <activity android:name=".Weekly10" />
        <activity android:name=".CatalogActivity"></activity><!-- ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.support.PARENT_ACTIVITY"
            android:value=".CatalogActivity" />
        <provider
            android:name=".data.DetailsProvider"
            android:authorities="com.example.keerthana.dailybud"
            android:exported="false"/>
    </application>

</manifest>